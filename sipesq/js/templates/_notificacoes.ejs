  <a href="#" class="dropdown-toggle" data-toggle="dropdown">
    Notificações
    <% if(data.count_new > 0) { %>
      <span class="badge badge-warning"><%= data.count_new %></span><b class="caret"></b>
    <%}%>
  </a>
  <ul class="dropdown-menu">
  <% for(i in data.items) { %>
    <li class="ntf <%= (data.items[i].read == true) ? '': 'notif-new'  %>">
      <a href="<%= data.items[i].notf_url %>">
      <div class="notif-content">
         <%= data.items[i].message %>
      </div>
      </a>
    </li>
  <%}%>
  <li class="divider"></li>
    <li>      
      <a href="/sipesq/index.php/notificacao" align="center"><b> Mais notificações...</b></a>
    </li>
</ul>
  